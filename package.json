    "node": ">=22.18 <23",
    "pnpm": ">=10.4 <11"
  },
  "packageManager": "pnpm@10.4.0",
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "current",
        "linux"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    },
    "onlyBuiltDependencies": [
